{
  "name": "DDRScoreTracker",
  "version": "0.2.0",
  "description": "Google Chrome extension for tracking your DanceDanceRevolution play records",
  "author": "Ryo Watanabe (https://github.com/ryowatanabe)",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.22.4",
    "babel-jest": "^29.5.0",
    "copy-webpack-plugin": "^6.4.1",
    "cross-env": "^7.0.2",
    "css-loader": "^5.2.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.14.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": ">=13",
    "prettier": "^2.8.8",
    "vue": "^3.3.4",
    "vue-loader": "^17.2.2",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.10.0",
    "webpack-extension-reloader": "^1.1.4"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint --ext .js,.vue src",
    "prettier": "prettier \"src/**/*.{js,vue,json,html}\" \"test/**/*.js\"",
    "prettier:write": "yarn prettier --write",
    "build": "yarn clean; cross-env NODE_ENV=production webpack",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "watch": "yarn build --watch",
    "watch:dev": "cross-env HMR=true yarn build:dev --watch",
    "test": "jest",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.{json,html}": "prettier --write",
    "test/**/*.js": "prettier --write"
  },
  "dependencies": {}
}
